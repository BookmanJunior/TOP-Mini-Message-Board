<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% messages.forEach(message => { %>
    <div class="user-info">
      <h2><%= message.user %></h2>
      <p><%= message.added%></p>
    </div>
    <p><%= message.text%></p>
    <% }) %>
  </body>
</html>
